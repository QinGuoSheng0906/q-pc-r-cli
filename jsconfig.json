{
   "compilerOptions": {
     "baseUrl": "./",
     "paths": {
       "@/*": ["src/*"],
       "@components/*": ["src/components/*"],
       "@actions/*": ["src/stores/actions/*"],
       "@assets/*": ["src/assets/*"],
       "@lib/*": ["src/lib/*"],
       "@pages/*": ["src/pages/*"],
     },
     "target": "ES6",
     "module": "commonjs",
     "jsx": "react",
     "allowSyntheticDefaultImports": true
   },
   "include": [
     "src/**/*"
   ],
   "exclude": ["node_modules"]
}
